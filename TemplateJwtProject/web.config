<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" 
                  arguments=".\TemplateJwtProject.dll" 
                  stdoutLogEnabled="false" 
                  stdoutLogFile=".\logs\stdout" 
                  hostingModel="inprocess" />
      <!-- 
        To enable logging for troubleshooting, set stdoutLogEnabled="true" above.
        Environment variables can be set at the IIS Application level or here.
        Remove the comment below to set a specific environment:
      <aspNetCore processPath="dotnet" arguments=".\TemplateJwtProject.dll" 
                  stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" 
                  hostingModel="inprocess">
        <environmentVariables>
          <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Production" />
        </environmentVariables>
      </aspNetCore>
      -->
      <httpErrors existingResponse="PassThrough" />
    </system.webServer>
  </location>
</configuration>
