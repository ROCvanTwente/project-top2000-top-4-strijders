<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" 
                  arguments="TemplateJwtProject.dll" 
                  stdoutLogEnabled="false" 
                  stdoutLogFile="logs\stdout" 
                  hostingModel="inprocess">
        <!-- 
          Uncomment to set a specific environment (e.g., Development, Staging, Production):
          <environmentVariables>
            <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Production" />
          </environmentVariables>
          
          To enable diagnostic logging, set stdoutLogEnabled="true" above.
          Note: Logging can impact performance and consume disk space.
        -->
      </aspNetCore>
      <httpErrors existingResponse="PassThrough" />
    </system.webServer>
  </location>
</configuration>
